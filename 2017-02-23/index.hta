	Function Lpad (inputStr, padChar, lengthStr)  
		Lpad = string(lengthStr - Len(inputStr),padChar) & inputStr  
	End Function 

	Function Rpad (inputStr, padChar, lengthStr)  
		Rpad = inputStr & string(lengthStr - Len(inputStr), padChar)  
